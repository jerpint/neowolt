{"id":"digest-gecev3xp","type":"spark","title":"nw digest · feb 25","timestamp":"2026-02-25T04:42:24.694Z","html":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>nw digest · feb 25</title>\n<style>\n  * { box-sizing: border-box; margin: 0; padding: 0; }\n  html, body { height: 100%; background: #0d1117; color: #c9d1d9;\n    font-family: 'SF Mono','Fira Code',monospace; }\n  body { display: flex; flex-direction: column; }\n  #content { flex: 1; overflow-y: auto; padding: 1rem 1rem 0.5rem; }\n  .date { font-size: 0.55rem; color: #333; letter-spacing: 0.2em;\n    text-transform: uppercase; margin-bottom: 1rem; text-align: center; }\n\n  /* HERO */\n  .hero { display: block; text-decoration: none; border-radius: 10px; overflow: hidden;\n    position: relative; margin-bottom: 0.75rem; border: 1px solid #1e2d1e;\n    opacity: 0; animation: rise 0.5s ease forwards; }\n  .hero-img-wrap { width: 100%; aspect-ratio: 2 / 1; overflow: hidden; position: relative; }\n  .hero-img-wrap img { width: 100%; height: 100%; object-fit: cover; object-position: center top;\n    display: block; filter: brightness(0.55); }\n  .hero-overlay { position: absolute; inset: 0; padding: 1rem;\n    background: linear-gradient(to top, #0d1117f0 35%, transparent 100%);\n    display: flex; flex-direction: column; justify-content: flex-end; }\n  .hero .badge { font-size: 0.55rem; color: #6b9; letter-spacing: 0.18em;\n    text-transform: uppercase; margin-bottom: 0.3rem; }\n  .hero h2 { font-size: 0.95rem; color: #e6edf3; font-weight: 600; line-height: 1.3; margin-bottom: 0.25rem; }\n  .hero .why { font-size: 0.68rem; color: #8b9a8b; line-height: 1.5; }\n\n  /* GRID */\n  .grid { display: grid; grid-template-columns: 1fr 1fr; gap: 0.6rem; margin-bottom: 0.75rem; }\n  .card { display: block; text-decoration: none; border-radius: 8px; overflow: hidden;\n    border: 1px solid #161b22; opacity: 0; animation: rise 0.5s ease forwards;\n    transition: border-color 0.15s; }\n  .card:hover { border-color: #2a3a2a; }\n  .card-img-wrap { width: 100%; aspect-ratio: 2 / 1; overflow: hidden; }\n  .card-img-wrap img { width: 100%; height: 100%; object-fit: cover; object-position: center top;\n    display: block; filter: brightness(0.65) saturate(0.8); }\n  .card-body { padding: 0.5rem 0.65rem; }\n  .card .tag { font-size: 0.53rem; letter-spacing: 0.13em; text-transform: uppercase;\n    margin-bottom: 0.2rem; opacity: 0.7; }\n  .card h3 { font-size: 0.73rem; color: #c9d1d9; line-height: 1.3; font-weight: 600; margin-bottom: 0.15rem; }\n  .card .blurb { font-size: 0.6rem; color: #555; line-height: 1.4; }\n\n  /* PAPERS CAROUSEL */\n  .section-label { font-size: 0.53rem; color: #333; letter-spacing: 0.18em;\n    text-transform: uppercase; margin-bottom: 0.5rem; }\n  .paper-carousel {\n    border: 1px solid #161b22; border-radius: 8px; overflow: hidden;\n    margin-bottom: 0.75rem;\n    opacity: 0; animation: rise 0.5s ease forwards;\n  }\n  .paper-track { display: flex; transition: transform 0.3s ease; }\n  .paper-slide {\n    flex: 0 0 100%; padding: 0.85rem 1rem;\n    display: flex; flex-direction: column; gap: 0.5rem;\n    min-height: 110px;\n  }\n  .paper-slide h4 { font-size: 0.78rem; color: #c9d1d9; line-height: 1.3; font-weight: 600; }\n  .paper-slide .abstract { font-size: 0.63rem; color: #666; line-height: 1.55; flex: 1; }\n  .paper-slide a { font-size: 0.58rem; color: #3a5a3a; text-decoration: none;\n    align-self: flex-start; }\n  .paper-slide a:hover { color: #6b9; }\n  .paper-nav {\n    display: flex; align-items: center; justify-content: space-between;\n    padding: 0.4rem 0.75rem; border-top: 1px solid #161b22; background: #0a0f15;\n  }\n  .pnav-btn { background: none; border: 1px solid #21262d; border-radius: 4px;\n    color: #555; cursor: pointer; font-size: 0.75rem;\n    width: 24px; height: 24px; display: flex; align-items: center; justify-content: center;\n    transition: all 0.15s; }\n  .pnav-btn:hover { border-color: #6b9; color: #6b9; }\n  .paper-dots { display: flex; gap: 0.3rem; }\n  .pdot { width: 5px; height: 5px; border-radius: 50%; background: #21262d;\n    cursor: pointer; transition: background 0.2s; }\n  .pdot.active { background: #6b9; }\n  .paper-counter { font-size: 0.55rem; color: #333; }\n\n  /* MUSIC */\n  #player { flex-shrink: 0; background: #0d1117; border-top: 1px solid #161b22; padding: 0.7rem 1rem; }\n  #player-label { font-size: 0.53rem; color: #333; letter-spacing: 0.18em;\n    text-transform: uppercase; margin-bottom: 0.6rem; }\n  .carousel { display: flex; align-items: center; gap: 0.6rem; margin-bottom: 0.5rem; }\n  .car-btn { flex-shrink: 0; background: none; border: 1px solid #21262d;\n    border-radius: 4px; color: #555; cursor: pointer; font-size: 0.8rem;\n    width: 26px; height: 26px; display: flex; align-items: center; justify-content: center;\n    transition: all 0.15s; }\n  .car-btn:hover { border-color: #6b9; color: #6b9; }\n  .car-thumb { flex-shrink: 0; width: 72px; height: 40px; border-radius: 4px;\n    object-fit: cover; border: 1px solid #21262d; display: block; }\n  .car-track { flex: 1; min-width: 0; border: 1px solid #21262d; border-radius: 6px;\n    padding: 0.35rem 0.7rem; display: flex; align-items: center; justify-content: space-between; gap: 0.5rem; }\n  .car-track-name { font-size: 0.7rem; color: #c9d1d9; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }\n  .play-btn { flex-shrink: 0; background: none; border: 1px solid #21262d;\n    border-radius: 4px; color: #6b9; cursor: pointer; font-size: 0.65rem;\n    padding: 0.2rem 0.5rem; transition: all 0.15s; white-space: nowrap; }\n  .play-btn:hover { border-color: #6b9; background: rgba(102,187,102,0.08); }\n  .car-dots { display: flex; gap: 0.3rem; justify-content: center; margin-bottom: 0.5rem; }\n  .car-dot { width: 5px; height: 5px; border-radius: 50%; background: #21262d;\n    transition: background 0.2s; cursor: pointer; }\n  .car-dot.active { background: #6b9; }\n  #yt-frame { width: 100%; height: 0; border: none; border-radius: 6px;\n    transition: height 0.3s ease; background: #161b22; display: block; }\n  #yt-frame.open { height: 155px; }\n\n  @keyframes rise {\n    from { opacity: 0; transform: translateY(6px); }\n    to   { opacity: 1; transform: translateY(0); }\n  }\n</style>\n</head>\n<body>\n<div id=\"content\">\n  <div class=\"date\">nw digest · feb 25 2026</div>\n\n  <a class=\"hero\" href=\"https://www.inceptionlabs.ai/blog/introducing-mercury-2\" target=\"_blank\">\n    <div class=\"hero-img-wrap\">\n      <img src=\"https://framerusercontent.com/assets/xqKJszO4tzsVEusCSL14t11EJZg.png\" alt=\"Mercury 2\" />\n    </div>\n    <div class=\"hero-overlay\">\n      <div class=\"badge\">★ top pick</div>\n      <h2>Mercury 2 — reasoning via diffusion</h2>\n      <div class=\"why\">Generates tokens in parallel, not left-to-right. If this scales it changes what agents can do.</div>\n    </div>\n  </a>\n\n  <div class=\"grid\">\n    <a class=\"card\" href=\"https://github.com/generalaction/emdash\" target=\"_blank\" style=\"animation-delay:0.1s\">\n      <div class=\"card-img-wrap\"><img src=\"https://opengraph.githubassets.com/552a7a4395498062f0af96a260cac5029287dd9e49d1a520b3a12c3a60f9b4bb/generalaction/emdash\" alt=\"Emdash\" /></div>\n      <div class=\"card-body\"><div class=\"tag\" style=\"color:#61afef\">agents</div><h3>Emdash</h3><div class=\"blurb\">Open-source agentic dev env.</div></div>\n    </a>\n    <a class=\"card\" href=\"https://github.com/huggingface/skills\" target=\"_blank\" style=\"animation-delay:0.15s\">\n      <div class=\"card-img-wrap\"><img src=\"https://opengraph.githubassets.com/9fa3c32b79659640adf1a29b7f7c0d00148599b3e6196cadcafcfd52742e5eb0/huggingface/skills\" alt=\"HF Skills\" /></div>\n      <div class=\"card-body\"><div class=\"tag\" style=\"color:#f0883e\">hugging face</div><h3>HF Skills</h3><div class=\"blurb\">Their \"skills\" system — same word as wolts.</div></div>\n    </a>\n    <a class=\"card\" href=\"https://pi.dev\" target=\"_blank\" style=\"animation-delay:0.2s\">\n      <div class=\"card-img-wrap\"><img src=\"https://pi.dev/social.png\" alt=\"Pi\" /></div>\n      <div class=\"card-body\"><div class=\"tag\" style=\"color:#6b9\">terminal</div><h3>Pi</h3><div class=\"blurb\">Minimal terminal coding harness.</div></div>\n    </a>\n    <a class=\"card\" href=\"https://github.com/moonshine-ai/moonshine\" target=\"_blank\" style=\"animation-delay:0.25s\">\n      <div class=\"card-img-wrap\"><img src=\"https://opengraph.githubassets.com/cbab8bb507c74327234cfa62d92c2557f7511c7cbf8333da279a945d9dca1c48/moonshine-ai/moonshine\" alt=\"Moonshine\" /></div>\n      <div class=\"card-body\"><div class=\"tag\" style=\"color:#c678dd\">open weights</div><h3>Moonshine STT</h3><div class=\"blurb\">Better than Whisper Large v3.</div></div>\n    </a>\n  </div>\n\n  <div class=\"section-label\">hf papers</div>\n  <div class=\"paper-carousel\" style=\"animation-delay:0.3s\">\n    <div class=\"paper-track\" id=\"paper-track\">\n      \n      <div class=\"paper-slide\">\n        <h4>The Art of Efficient Reasoning</h4>\n        <div class=\"abstract\">Shifts LLM evaluation from passive perception to active problem-solving. Better reasoning without just scaling compute — data, reward, and optimization strategies that actually work.</div>\n        <a href=\"https://huggingface.co/papers/2602.21015\" target=\"_blank\">read on huggingface →</a>\n      </div>\n      <div class=\"paper-slide\">\n        <h4>Implicit Intelligence</h4>\n        <div class=\"abstract\">Evaluating agents on what users don't say. Training terminal agents with optimized data strategies — filtering, curriculum learning, long context. Directly the problem of building an agent that knows what you want without being told.</div>\n        <a href=\"https://huggingface.co/papers/2602.21193\" target=\"_blank\">read on huggingface →</a>\n      </div>\n      <div class=\"paper-slide\">\n        <h4>PyVision-RL: Agentic Vision via RL</h4>\n        <div class=\"abstract\">Open agentic vision models trained with reinforcement learning. Addresses \"interaction collapse\" in multimodal models — agents that can see and act, not just describe.</div>\n        <a href=\"https://huggingface.co/papers/2602.20739\" target=\"_blank\">read on huggingface →</a>\n      </div>\n      <div class=\"paper-slide\">\n        <h4>LongCLI-Bench</h4>\n        <div class=\"abstract\">Benchmark for long-horizon agentic programming in CLIs. State-of-the-art agents hit below 20% pass rate on complex terminal tasks. Puts a number on where we actually are.</div>\n        <a href=\"https://huggingface.co/papers/2602.14337\" target=\"_blank\">read on huggingface →</a>\n      </div>\n      <div class=\"paper-slide\">\n        <h4>Reflective Test-Time Planning</h4>\n        <div class=\"abstract\">LLM-powered robots that learn from mistakes mid-task via reflection-in-action and reflection-on-action. Household and manipulation benchmarks improved. Agents that course-correct.</div>\n        <a href=\"https://huggingface.co/papers/2602.21198\" target=\"_blank\">read on huggingface →</a>\n      </div>\n    </div>\n    <div class=\"paper-nav\">\n      <button class=\"pnav-btn\" onclick=\"pstep(-1)\">&#8592;</button>\n      <div class=\"paper-dots\" id=\"paper-dots\"></div>\n      <span class=\"paper-counter\" id=\"paper-counter\"></span>\n      <button class=\"pnav-btn\" onclick=\"pstep(1)\">&#8594;</button>\n    </div>\n  </div>\n</div>\n\n<div id=\"player\">\n  <div id=\"player-label\">vibes</div>\n  <div class=\"carousel\">\n    <button class=\"car-btn\" onclick=\"step(-1)\">&#8592;</button>\n    <img class=\"car-thumb\" id=\"car-thumb\" src=\"\" alt=\"\" />\n    <div class=\"car-track\">\n      <span class=\"car-track-name\" id=\"track-name\"></span>\n      <button class=\"play-btn\" id=\"play-btn\" onclick=\"playCurrentTrack()\">&#9654; play</button>\n    </div>\n    <button class=\"car-btn\" onclick=\"step(1)\">&#8594;</button>\n  </div>\n  <div class=\"car-dots\" id=\"car-dots\"></div>\n  <iframe id=\"yt-frame\" allow=\"autoplay; encrypted-media\" allowfullscreen></iframe>\n</div>\n\n<script>\n// --- Papers carousel ---\nconst paperCount = 5;\nlet pCurrent = 0;\nconst pTrack = document.getElementById('paper-track');\nconst pDotsEl = document.getElementById('paper-dots');\nconst pCounter = document.getElementById('paper-counter');\n\nfor (let i = 0; i < paperCount; i++) {\n  const d = document.createElement('div');\n  d.className = 'pdot' + (i === 0 ? ' active' : '');\n  d.onclick = () => pGo(i);\n  pDotsEl.appendChild(d);\n}\n\nfunction pGo(i) {\n  pCurrent = (i + paperCount) % paperCount;\n  pTrack.style.transform = 'translateX(-' + (pCurrent * 100) + '%)';\n  document.querySelectorAll('.pdot').forEach((d,j) => d.classList.toggle('active', j === pCurrent));\n  pCounter.textContent = (pCurrent + 1) + ' / ' + paperCount;\n}\nfunction pstep(dir) { pGo(pCurrent + dir); }\npGo(0);\n\n// --- Music carousel ---\nconst tracks = [\n  { name: 'QOTSA — No One Knows',              id: 's88r_q7oufE' },\n  { name: 'QOTSA — Go with the Flow',           id: 'DcHKOC64KnE' },\n  { name: 'Khruangbin — A Calf Born in Winter', id: 'My2q6F0Dprk' },\n  { name: 'Khruangbin — Time (You and I)',       id: 'oc50wHexbwg' },\n];\nlet current = 0, playing = false;\nconst nameEl   = document.getElementById('track-name');\nconst dotsEl   = document.getElementById('car-dots');\nconst frame    = document.getElementById('yt-frame');\nconst playBtn  = document.getElementById('play-btn');\nconst thumbEl  = document.getElementById('car-thumb');\n\ntracks.forEach((_, i) => {\n  const d = document.createElement('div');\n  d.className = 'car-dot' + (i === 0 ? ' active' : '');\n  d.onclick = () => go(i);\n  dotsEl.appendChild(d);\n});\n\nfunction go(i) {\n  current = (i + tracks.length) % tracks.length;\n  nameEl.textContent = tracks[current].name;\n  thumbEl.src = 'https://img.youtube.com/vi/' + tracks[current].id + '/mqdefault.jpg';\n  document.querySelectorAll('.car-dot').forEach((d,j) => d.classList.toggle('active', j === current));\n  if (playing) loadTrack(current);\n}\nfunction step(dir) { go(current + dir); }\nfunction loadTrack(i) {\n  frame.src = 'https://www.youtube.com/embed/' + tracks[i].id + '?autoplay=1';\n  frame.classList.add('open');\n  playBtn.textContent = '■ stop';\n  playing = true;\n}\nfunction playCurrentTrack() {\n  if (playing) {\n    frame.src = ''; frame.classList.remove('open');\n    playBtn.textContent = '▶ play'; playing = false;\n  } else { loadTrack(current); }\n}\ngo(0);\n</script>\n</body>\n</html>"}